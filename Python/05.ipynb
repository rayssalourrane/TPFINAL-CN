   "source": [
    "from math import pi as pi\n",
    "from timeit import default_timer as timer\n",
    "Euler = 0\n",
    "t_ini = timer()\n",
    "for n in range(1,10000000):\n",
    "    Euler += 1/(n**2)\n",
    "t_fim = timer()\n",
    "print(f'Euler: {Euler} == \\u03C0^2/6: {pi**2/6}')\n",
    "print(f\"Tempo: {t_fim-t_ini} segundos\")"
   ]
  }
